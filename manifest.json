{
    "name": "Farhan's todo app",
    "version": "0.0.1",
    "manifest_version": 2, 
  
  "description": "This extension will open a new tab for Farhan's To-Do Application",
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "logo.png"
  },
  "icons": {
    "128": "logo.png"
  },
  "permissions": ["tabs"]
}